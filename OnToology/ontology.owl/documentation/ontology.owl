     -->


    


    <!-- http://www.semanticweb.org/mmasser95/ontologies/mmasser95/ontoFlightRatings#hasAnswered -->


    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/mmasser95/ontologies/mmasser95/ontoFlightRatings#hasAnswered">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/mmasser95/ontologies/mmasser95/ontoFlightRatings#isAnsweredBy"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/mmasser95/ontologies/mmasser95/ontoFlightRatings#Passenger"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/mmasser95/ontologies/mmasser95/ontoFlightRatings#Review"/>
        <rdfs:comment xml:lang="en">This property is used to relate a passenger who has answered a review within the ontology.</rdfs:comment>
        <rdfs:label xml:lang="en">has Answered</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/mmasser95/ontologies/mmasser95/ontoFlightRatings#hasDestination -->


    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/mmasser95/ontologies/mmasser95/ontoFlightRatings#hasDestination">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/mmasser95/ontologies/mmasser95/ontoFlightRatings#isDestinationOf"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/mmasser95/ontologies/mmasser95/ontoFlightRatings#Flight"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/mmasser95/ontologies/mmasser95/ontoFlightRatings#Airport"/>
        <rdfs:comment xml:lang="en">This property is used to relate a flight entity with the destination airport within the ontology. It serves as a means of establishing a relationship between a specific flight instance and the airport entity representing its destination.</rdfs:comment>
        <rdfs:label xml:lang="en">has Destination</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/mmasser95/ontologies/mmasser95/ontoFlightRatings#hasFrom -->


    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/mmasser95/ontologies/mmasser95/ontoFlightRatings#hasFrom">
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/mmasser95/ontologies/mmasser95/ontoFlightRatings#isFrom"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/mmasser95/ontologies/mmasser95/ontoFlightRatings#Country"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/mmasser95/ontologies/mmasser95/ontoFlightRatings#Passenger"/>
        <rdfs:comment xml:lang="en">This property is the inverse of isFrom</rdfs:comment>
        <rdfs:label xml:lang="en">has From</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/mmasser95/ontologies/mmasser95/ontoFlightRatings#hasOrigin -->


    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/mmasser95/ontologies/mmasser95/ontoFlightRatings#hasOrigin">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/mmasser95/ontologies/mmasser95/ontoFlightRatings#isOriginOf"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/mmasser95/ontologies/mmasser95/ontoFlightRatings#Flight"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/mmasser95/ontologies/mmasser95/ontoFlightRatings#Airport"/>
        <rdfs:comment xml:lang="en">This property is used to relate a flight entity with the origin airport within the ontology. It serves as a means of establishing a relationship between a specific flight instance and the airport entity representing its point of origin.</rdfs:comment>
        <rdfs:label xml:lang="en">has Origin</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/mmasser95/ontologies/mmasser95/ontoFlightRatings#hasReviewed -->


    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/mmasser95/ontologies/mmasser95/ontoFlightRatings#hasReviewed">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/mmasser95/ontologies/mmasser95/ontoFlightRatings#isReviewedBy"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/mmasser95/ontologies/mmasser95/ontoFlightRatings#Review"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/mmasser95/ontologies/mmasser95/ontoFlightRatings#Flight"/>
        <rdfs:comment xml:lang="en">This property is used to relate a review with the flight it evaluates within the ontology.</rdfs:comment>
        <rdfs:label xml:lang="en">hasReviewed</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/mmasser95/ontologies/mmasser95/ontoFlightRatings#hasStopOver -->


    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/mmasser95/ontologies/mmasser95/ontoFlightRatings#hasStopOver">
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/mmasser95/ontologies/mmasser95/ontoFlightRatings#isStopOverOf"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/mmasser95/ontologies/mmasser95/ontoFlightRatings#Flight"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/mmasser95/ontologies/mmasser95/ontoFlightRatings#Airport"/>
        <rdfs:comment xml:lang="en">This property is used to relate a flight within an airport, reflecting that the flight has a stopover and the airport is where the passenger made the stopover.</rdfs:comment>
        <rdfs:label xml:lang="en">hasStopOver</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/mmasser95/ontologies/mmasser95/ontoFlightRatings#hasVehicle -->


    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/mmasser95/ontologies/mmasser95/ontoFlightRatings#hasVehicle">
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/mmasser95/ontologies/mmasser95/ontoFlightRatings#isVehicleOf"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/mmasser95/ontologies/mmasser95/ontoFlightRatings#Flight"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/mmasser95/ontologies/mmasser95/ontoFlightRatings#Aircraft"/>
        <rdfs:comment xml:lang="en">This property is used to relate the classes Flight and Aircraft within the ontology, indicating that the aircraft is used to operate the flight.</rdfs:comment>
        <rdfs:label xml:lang="en">has Vehicle</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/mmasser95/ontologies/mmasser95/ontoFlightRatings#isAnsweredBy -->


    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/mmasser95/ontologies/mmasser95/ontoFlightRatings#isAnsweredBy">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/mmasser95/ontologies/mmasser95/ontoFlightRatings#Review"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/mmasser95/ontologies/mmasser95/ontoFlightRatings#Passenger"/>
        <rdfs:comment xml:lang="en">Inverse property of hasAnswered.</rdfs:comment>
        <rdfs:label xml:lang="en">is Answered By</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/mmasser95/ontologies/mmasser95/ontoFlightRatings#isDestinationOf -->


    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/mmasser95/ontologies/mmasser95/ontoFlightRatings#isDestinationOf">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/mmasser95/ontologies/mmasser95/ontoFlightRatings#Airport"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/mmasser95/ontologies/mmasser95/ontoFlightRatings#Flight"/>
        <rdfs:comment xml:lang="en">Inverse property of hasDestination</rdfs:comment>
        <rdfs:label xml:lang="en">is Destination Of</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/mmasser95/ontologies/mmasser95/ontoFlightRatings#isFrom -->


    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/mmasser95/ontologies/mmasser95/ontoFlightRatings#isFrom">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/mmasser95/ontologies/mmasser95/ontoFlightRatings#Passenger"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/mmasser95/ontologies/mmasser95/ontoFlightRatings#Country"/>
        <rdfs:comment xml:lang="en">This property is used to relate a passenger with the country from which they originate within the ontology. It serves as a means of establishing a relationship between a passenger individual and the country entity representing their place of origin.</rdfs:comment>
        <rdfs:label xml:lang="en">is From</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/mmasser95/ontologies/mmasser95/ontoFlightRatings#isOriginOf -->


    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/mmasser95/ontologies/mmasser95/ontoFlightRatings#isOriginOf">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/mmasser95/ontologies/mmasser95/ontoFlightRatings#Airport"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/mmasser95/ontologies/mmasser95/ontoFlightRatings#Flight"/>
        <rdfs:comment xml:lang="en">Inverse property of hasOrigin.</rdfs:comment>
        <rdfs:label xml:lang="en">is Origin Of</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/mmasser95/ontologies/mmasser95/ontoFlightRatings#isReviewedBy -->


    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/mmasser95/ontologies/mmasser95/ontoFlightRatings#isReviewedBy">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/mmasser95/ontologies/mmasser95/ontoFlightRatings#Flight"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/mmasser95/ontologies/mmasser95/ontoFlightRatings#Review"/>
        <rdfs:comment xml:lang="en">Is the inverse of hasReviewed</rdfs:comment>
        <rdfs:label xml:lang="en">is Reviewed By</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/mmasser95/ontologies/mmasser95/ontoFlightRatings#isStopOverOf -->


    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/mmasser95/ontologies/mmasser95/ontoFlightRatings#isStopOverOf">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/mmasser95/ontologies/mmasser95/ontoFlightRatings#Airport"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/mmasser95/ontologies/mmasser95/ontoFlightRatings#Flight"/>
        <rdfs:comment xml:lang="en">This is the inverse of has StopOver.</rdfs:comment>
        <rdfs:label xml:lang="en">is StopOver Of</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/mmasser95/ontologies/mmasser95/ontoFlightRatings#isVehicleOf -->


    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/mmasser95/ontologies/mmasser95/ontoFlightRatings#isVehicleOf">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/mmasser95/ontologies/mmasser95/ontoFlightRatings#Aircraft"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/mmasser95/ontologies/mmasser95/ontoFlightRatings#Flight"/>
        <rdfs:comment xml:lang="en">This property is the inverse of has Vehicle</rdfs:comment>
        <rdfs:label xml:lang="en">is Vehicle</rdfs:label>
    </owl:ObjectProperty>
    


