    <!-- http://www.semanticweb.org/mmasser95/ontologies/mmasser95/ontoFlightRatings#aircraftBrand -->


    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/mmasser95/ontologies/mmasser95/ontoFlightRatings#aircraftBrand">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topDataProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/mmasser95/ontologies/mmasser95/ontoFlightRatings#Aircraft"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment xml:lang="en">This data property is used to store the brand of the aircraft.</rdfs:comment>
        <rdfs:label xml:lang="en">Aircraft Brand</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://www.semanticweb.org/mmasser95/ontologies/mmasser95/ontoFlightRatings#aircraftModel -->


    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/mmasser95/ontologies/mmasser95/ontoFlightRatings#aircraftModel">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topDataProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/mmasser95/ontologies/mmasser95/ontoFlightRatings#Aircraft"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment xml:lang="en">This data property is used to store the model of the aircraft</rdfs:comment>
        <rdfs:label xml:lang="en">Aircraft Model</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://www.semanticweb.org/mmasser95/ontologies/mmasser95/ontoFlightRatings#airportName -->


    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/mmasser95/ontologies/mmasser95/ontoFlightRatings#airportName">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topDataProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/mmasser95/ontologies/mmasser95/ontoFlightRatings#Airport"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment xml:lang="en">This data property is used to store the airport name</rdfs:comment>
        <rdfs:label xml:lang="en">Airport Name</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://www.semanticweb.org/mmasser95/ontologies/mmasser95/ontoFlightRatings#countryName -->


    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/mmasser95/ontologies/mmasser95/ontoFlightRatings#countryName">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topDataProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/mmasser95/ontologies/mmasser95/ontoFlightRatings#Country"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment xml:lang="en">This data property is used to store the name of the country</rdfs:comment>
        <rdfs:label xml:lang="en">Country Name</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://www.semanticweb.org/mmasser95/ontologies/mmasser95/ontoFlightRatings#flightDate -->


    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/mmasser95/ontologies/mmasser95/ontoFlightRatings#flightDate">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topDataProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/mmasser95/ontologies/mmasser95/ontoFlightRatings#Flight"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment xml:lang="en">This data property is used to store the date of the flight</rdfs:comment>
        <rdfs:label xml:lang="en">Flight Date</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://www.semanticweb.org/mmasser95/ontologies/mmasser95/ontoFlightRatings#personName -->


    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/mmasser95/ontologies/mmasser95/ontoFlightRatings#personName">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topDataProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/mmasser95/ontologies/mmasser95/ontoFlightRatings#Passenger"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment xml:lang="en">This data property is used to store the name of a passenger.</rdfs:comment>
        <rdfs:label xml:lang="en">Person Name</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://www.semanticweb.org/mmasser95/ontologies/mmasser95/ontoFlightRatings#reviewComment -->


    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/mmasser95/ontologies/mmasser95/ontoFlightRatings#reviewComment">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topDataProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/mmasser95/ontologies/mmasser95/ontoFlightRatings#Review"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment xml:lang="en">This data property is used to store the comment of a review.</rdfs:comment>
        <rdfs:label xml:lang="en">Review Comment</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://www.semanticweb.org/mmasser95/ontologies/mmasser95/ontoFlightRatings#reviewDate -->


    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/mmasser95/ontologies/mmasser95/ontoFlightRatings#reviewDate">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topDataProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/mmasser95/ontologies/mmasser95/ontoFlightRatings#Review"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment xml:lang="en">This data property is used to store the date of a review</rdfs:comment>
        <rdfs:label xml:lang="en">Review Date</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://www.semanticweb.org/mmasser95/ontologies/mmasser95/ontoFlightRatings#reviewRatingValue -->


    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/mmasser95/ontologies/mmasser95/ontoFlightRatings#reviewRatingValue">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topDataProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/mmasser95/ontologies/mmasser95/ontoFlightRatings#Review"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#float"/>
        <rdfs:comment xml:lang="en">This data property is used to store the value of the rating of a Review about a Flight.</rdfs:comment>
        <rdfs:label xml:lang="en">Review Rating Value</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://www.semanticweb.org/mmasser95/ontologies/mmasser95/ontoFlightRatings#reviewTitle -->


    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/mmasser95/ontologies/mmasser95/ontoFlightRatings#reviewTitle">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topDataProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/mmasser95/ontologies/mmasser95/ontoFlightRatings#Review"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment xml:lang="en">This data property is used to store the title of a review.</rdfs:comment>
        <rdfs:label xml:lang="en">Review Title</rdfs:label>
    </owl:DatatypeProperty>
    


