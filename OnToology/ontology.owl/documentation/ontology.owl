     -->


    


    <!-- http://www.semanticweb.org/mmasser95/ontologies/mmasser95/ontoFlightRatings#Aircraft -->


    <owl:Class rdf:about="http://www.semanticweb.org/mmasser95/ontologies/mmasser95/ontoFlightRatings#Aircraft">
        <rdfs:comment xml:lang="en">This class represents the aircraft that operate the flight within the ontology.</rdfs:comment>
        <rdfs:label xml:lang="en">Aircraft</rdfs:label>
        <rdfs:sameAs xml:lang="en">https://dbpedia.org/ontology/Aircraft</rdfs:sameAs>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/mmasser95/ontologies/mmasser95/ontoFlightRatings#Airport -->


    <owl:Class rdf:about="http://www.semanticweb.org/mmasser95/ontologies/mmasser95/ontoFlightRatings#Airport">
        <owl:disjointWith rdf:resource="http://www.semanticweb.org/mmasser95/ontologies/mmasser95/ontoFlightRatings#Country"/>
        <rdfs:comment xml:lang="en">This location represents an airport within the ontology.</rdfs:comment>
        <rdfs:label xml:lang="en">Airport</rdfs:label>
        <rdfs:sameAs xml:lang="en">https://dbpedia.org/ontology/Airport</rdfs:sameAs>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/mmasser95/ontologies/mmasser95/ontoFlightRatings#BadReview -->


    <owl:Class rdf:about="http://www.semanticweb.org/mmasser95/ontologies/mmasser95/ontoFlightRatings#BadReview">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/mmasser95/ontologies/mmasser95/ontoFlightRatings#TypeOfReview"/>
        <owl:disjointWith rdf:resource="http://www.semanticweb.org/mmasser95/ontologies/mmasser95/ontoFlightRatings#GoodReview"/>
        <rdfs:comment xml:lang="en">This class is used to classify a review as &quot;bad&quot; within the ontology, based on the rating value or recommendation provided by the passenger.</rdfs:comment>
        <rdfs:label xml:lang="en">BadReview</rdfs:label>
    </owl:Class>
    


